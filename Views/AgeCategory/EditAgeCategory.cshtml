@model KidsCenterA.Models.AgeCategoryModel

@{
    ViewBag.Title = "EditAgeCategory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>EditAgeCategory</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>AgeCategoryModel</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.AgeCategoryId, htmlAttributes: new { @class = "control-label col-md-2" })
